[loggers]
keys=root, auth, db, routers

[handlers]
keys=console, file

[formatters]
keys=api

[logger_root]
level=DEBUG
handlers=console, file
propagate=1

[logger_auth]
level=DEBUG
handlers=console, file
qualname=au

[logger_db]
level=DEBUG
handlers=console, file
qualname=db

[logger_routers]
level=DEBUG
handlers=console, file
qualname=routers

[handler_console]
class=StreamHandler
formatter=api
args=(sys.stdout,)

[handler_file]
class=FileHandler
formatter=api
args=('api.log', 'w')

[formatter_api]
format=[%(name)s]:[%(process)d][%(processName)s][%(name)s]:%(asctime)s [%(levelname)s] %(message)s

